				function()
					if toggle == true then
						toggle = false
					else
						toggle = true
					end
					if toggle == true then
						script.Parent.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
					end
					if toggle == false then
						script.Parent.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
					end

					if toggle == true then
						--true here
					end
					if toggle == false then
						--false here
					end
				end
			)
		end
